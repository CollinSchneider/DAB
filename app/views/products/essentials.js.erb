// Update pagination link
<% if @essentials.current_page === @essentials.total_pages %>
  $('.pagination').remove();
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@essentials, 'Next Page', :remote => true) %>");
<% end %>
