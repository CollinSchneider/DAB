$("<%=j render(:partial => 'apparel') %>")
.appendTo($(".infinite-table"));

// Update pagination link
<% if @apparel.current_page === @apparel.total_pages %>
  $('.pagination').remove();
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@apparel, 'Next Page', :remote => true) %>");
<% end %>
