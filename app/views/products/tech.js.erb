
  $("<%=j render(:partial => 'tech') %>")
  .appendTo($(".infinite-table"));


// Update pagination link
<% if @tech.current_page === @tech.total_pages %>
  $('.pagination').remove();
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@tech, 'Next Page', :remote => true) %>");
<% end %>
