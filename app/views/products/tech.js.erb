var rendered;
function loadProducts(){
  rendered = $("<%=j render(:partial => 'tech') %>")
};
function appendProducts(){
  rendered.appendTo($('.infinite-table'))
}
loadProducts()
setTimeout(appendProducts, 200);
// Update pagination link
<% if @tech.current_page === @tech.total_pages %>
  $('.pagination').remove();
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@tech, '', :remote => true) %>");
<% end %>
