
  $("<%=j render(:partial => 'accessory') %>")
  .appendTo($(".infinite-table"));

// Update pagination link
<% if @accessories.current_page === @accessories.total_pages %>
  $('.pagination').remove();
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@accessories, 'Next Page', :remote => true) %>");
<% end %>
